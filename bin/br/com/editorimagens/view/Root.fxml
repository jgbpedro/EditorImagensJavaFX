      <Button fx:id="btnProximo" disable="true" layoutX="103.0" layoutY="340.0" mnemonicParsing="false" onAction="#carregarProximo" text="Próximo" />
      <Button fx:id="btnAnterior" disable="true" layoutX="29.0" layoutY="340.0" mnemonicParsing="false" onAction="#carregarAnterior" text="Anterior" />
      <Label layoutX="401.0" layoutY="28.0" text="Contraste" />

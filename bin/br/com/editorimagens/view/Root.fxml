      <Slider fx:id="sldBrilho" layoutX="357.0" layoutY="101.0" />
      <Label layoutX="401.0" layoutY="78.0" text="Brilho" />
      <Slider fx:id="sldSaturacao" layoutX="357.0" layoutY="158.0" />
      <Label layoutX="401.0" layoutY="134.0" text="Saturação" />
      <Slider fx:id="sldTonalidade" layoutX="357.0" layoutY="205.0" />
